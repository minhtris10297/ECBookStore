@model Model.EntityFramework.Sach
@using Common

@{
    ViewBag.Title = "BookDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var relatedProduct = (List<Model.EntityFramework.Sach>)ViewBag.RelatedProduct;
    var Comment = (List<Model.EntityFramework.DanhGiaCuaCustomer>)ViewBag.comment;
    var ktlogin = ViewBag.ktdangnhap;
    var muahang = ViewBag.muahang;
    var danhanxet = ViewBag.DaNhanXet;

    var countStar = ViewBag.countStar;
    var sumStar = ViewBag.sumStar;
    int rating;
    if (countStar == 0)
    {
        rating = 0;
    }
    else { rating = (int)(sumStar / countStar); }
}

<!-- breadcrumb -->
<div class="container">
    <div class="bread-crumb flex-w p-l-25 p-r-15 p-t-30 p-lr-0-lg">
        <a href="/Assets/Customer/cozastore/index.html" class="stext-109 cl8 hov-cl1 trans-04">
            Home
            <i class="fa fa-angle-right m-l-9 m-r-10" aria-hidden="true"></i>
        </a>

        <a href="/Assets/Customer/cozastore/product.html" class="stext-109 cl8 hov-cl1 trans-04">
            @Model.TheLoai.TenTheLoai
            <i class="fa fa-angle-right m-l-9 m-r-10" aria-hidden="true"></i>
        </a>

        <span class="stext-109 cl4">
            @Model.TenSach
        </span>
    </div>
</div>


<!-- Product Detail -->
<section class="sec-product-detail bg0 p-t-65 p-b-60">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-lg-7 p-b-30">
                <div class="p-l-25 p-r-30 p-lr-0-lg">
                    <div class="wrap-slick3 flex-sb flex-w">
                        <div class="wrap-slick3-dots"></div>
                        <div class="wrap-slick3-arrows flex-sb-m flex-w"></div>

                        <div class="slick3 gallery-lb">
                            <div class="item-slick3" data-thumb="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,1)">
                                <div class="wrap-pic-w pos-relative">
                                    <img src="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,1)" alt="IMG-PRODUCT">

                                    <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,1)">
                                        <i class="fa fa-expand"></i>
                                    </a>
                                </div>
                            </div>

                            <div class="item-slick3" data-thumb="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,2)">
                                <div class="wrap-pic-w pos-relative">
                                    <img src="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,2)" alt="IMG-PRODUCT">

                                    <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,2)">
                                        <i class="fa fa-expand"></i>
                                    </a>
                                </div>
                            </div>

                            <div class="item-slick3" data-thumb="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,3)">
                                <div class="wrap-pic-w pos-relative">
                                    <img src="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,3)" alt="IMG-PRODUCT">

                                    <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="@HtmlHelperExtensions.ImageBookOrDefault(@Model.SachID,3)">
                                        <i class="fa fa-expand"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-md-6 col-lg-5 p-b-30">
                <div class="p-r-50 p-t-5 p-lr-0-lg">
                    <h4 class="mtext-105 cl2 js-name-detail p-b-14">
                        @Model.TenSach
                    </h4>


                    <span class="fs-18 cl11">

                        @if (countStar == 0)
                        {
                            <a>
                                (Chưa có lượt đánh giá)
                            </a>
                        }
                        else
                        {
                            for (int i = 0; i < 5; i++)
                            {
                                if (rating > 0)
                                {
                                    <i class="zmdi zmdi-star"></i>
                                    rating--;
                                }
                                else
                                {
                                    <i class="zmdi zmdi-star-outline"></i>
                                }
                            }
                            <a>
                                (Có @countStar đánh giá)
                            </a>
                        }

                    </span>
                    @if (Model.GiaKhuyenMai == null)
                    {
                        <p style="color: red">
                            @Model.GiaTien.ToString("N0") đồng
                        </p>
                    }
                    else
                    {
                        <p style="color: red">
                            @Model.GiaKhuyenMai.GetValueOrDefault(0).ToString("N0") đồng
                        </p>
                    }

                    @if (Model.GiaKhuyenMai != null)
                    {
                        <p>
                            Tiết kiệm: <a style="color: red">@HtmlHelperExtensions.ConvertDecimaltoIntegerSalePrice(Model.GiaKhuyenMai.GetValueOrDefault(0), Model.GiaTien) %<a> @(Model.GiaTien - Model.GiaKhuyenMai) ₫
                        </p>
                        <p>
                            Giá thị trường: @Model.GiaTien.ToString("N0") ₫
                        </p>
                    }

                    <!--  -->
                    <div class="p-t-33">

                        <div class="flex-w flex-r-m p-b-10">
                            <div class="size-204 flex-w flex-m respon6-next">
                                @using (Html.BeginForm("AddItem", "Cart", new { id = Model.SachID }, FormMethod.Post))
                                {
                                    <div class="wrap-num-product flex-w m-r-20 m-tb-10">
                                        <div class="btn-num-product-down cl8 hov-btn3 trans-04 flex-c-m">
                                            <i class="fs-16 zmdi zmdi-minus"></i>
                                        </div>

                                        <input class="mtext-104 cl3 txt-center num-product" type="number" id="quantity" name="quantity" value="1" max="@ViewBag.QuantityMax">

                                        <div class="btn-num-product-up cl8 hov-btn3 trans-04 flex-c-m">
                                            <i class="fs-16 zmdi zmdi-plus"></i>
                                        </div>
                                    </div>
                                    @*Lay cai quantity tu input num*@

                                if (Model.SoLuong > 0)
                                {
                                    <button class="flex-c-m stext-101 cl0 size-101 bg1 bor1 hov-btn1 p-lr-15 trans-04 js-addcart-detail" type="submit">
                                        Chọn mua
                                    </button>
                                }
                                else
                                {
                                    <span class="btn-default">
                                        Hết hàng
                                    </span>
                                    }
                                }

                            </div>
                        </div>
                    </div>

                    <!--  -->
                    <div class="flex-w flex-m p-l-100 p-t-40 respon7">
                        <div class="flex-m bor9 p-r-10 m-r-11">
                            <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 js-addwish-detail tooltip100" data-tooltip="Thêm vào ưu thích">
                                <i class="zmdi zmdi-favorite"></i>
                            </a>
                        </div>

                        <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 m-r-8 tooltip100" data-tooltip="Facebook">
                            <i class="fa fa-facebook"></i>
                        </a>

                        <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 m-r-8 tooltip100" data-tooltip="Twitter">
                            <i class="fa fa-twitter"></i>
                        </a>

                        <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 m-r-8 tooltip100" data-tooltip="Google Plus">
                            <i class="fa fa-google-plus"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <!-- Related Products -->
        <section class="sec-relate-product bg0 p-t-45 p-b-105">
            <div class="container">
                <div class="p-b-45">
                    <h3 class="txt-left">
                        SẢN PHẨM THƯỜNG ĐƯỢC XEM CÙNG
                    </h3>
                </div>

                <!-- Slide2 -->
                <div class="wrap-slick2">
                    <div class="slick2">
                        @foreach (var item in relatedProduct)
                        {
                            if (@item.SachID != @ViewBag.BookId)
                            {
                                <div class="item-slick2 p-l-15 p-r-15 p-t-15 p-b-15">
                                    <!-- Block2 -->
                                    <div class="block2">
                                        <div class="block2-pic hov-img0">
                                            <img src="@HtmlHelperExtensions.ImageBookOrDefault(@item.SachID,1)" alt="IMG-PRODUCT">

                                            <a href="/Assets/Customer/cozastore/#" class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04 js-show-modal1" data-val="Sach@(item.SachID)">
                                                Xem nhanh
                                            </a>
                                        </div>

                                        <div class="block2-txt flex-w flex-t p-t-14">
                                            <div class="block2-txt-child1 flex-col-l ">
                                                <a href="@Url.Action("BookDetail","ListBooks",new {id = @item.SachID})" class="stext-104 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
                                                    @item.TenSach
                                                </a>

                                                <span class="stext-105 cl3">
                                                    @item.GiaTien
                                                </span>
                                            </div>

                                            <div class="block2-txt-child2 flex-r p-t-3">
                                                <a href="/Assets/Customer/cozastore/#" class="btn-addwish-b2 dis-block pos-relative js-addwish-b2">
                                                    <img class="icon-heart1 dis-block trans-04" src="/Assets/Customer/cozastore/images/icons/icon-heart-01.png" alt="ICON">
                                                    <img class="icon-heart2 dis-block trans-04 ab-t-l" src="/Assets/Customer/cozastore/images/icons/icon-heart-02.png" alt="ICON">
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }

                        }



                    </div>
                </div>
            </div>
        </section>

        @section ModalProduct
{
            @foreach (var item in relatedProduct)
    {
        <!-- Modal1 -->
                <div class="wrap-modal1 js-modal1 p-t-60 p-b-20" id="Sach@(item.SachID)">
                    <div class="overlay-modal1 js-hide-modal1"></div>

                    <div class="container">
                        <div class="bg0 p-t-60 p-b-30 p-lr-15-lg how-pos3-parent">
                            <button class="how-pos3 hov3 trans-04 js-hide-modal1">
                                <img src="/Assets/Customer/cozastore/images/icons/icon-close.png" alt="CLOSE">
                            </button>

                            <div class="row">
                                <div class="col-md-6 col-lg-7 p-b-30">
                                    <div class="p-l-25 p-r-30 p-lr-0-lg">
                                        <div class="wrap-slick3 flex-sb flex-w">
                                            <div class="wrap-slick3-dots"></div>
                                            <div class="wrap-slick3-arrows flex-sb-m flex-w"></div>

                                            <div class="slick3 gallery-lb">
                                                <div class="item-slick3" data-thumb="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,1)">
                                                    <div class="wrap-pic-w pos-relative">
                                                        <img src="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,1)" alt="IMG-PRODUCT">

                                                        <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,1)">
                                                            <i class="fa fa-expand"></i>
                                                        </a>
                                                    </div>
                                                </div>

                                                <div class="item-slick3" data-thumb="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,2)">
                                                    <div class="wrap-pic-w pos-relative">
                                                        <img src="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,2)" alt="IMG-PRODUCT">

                                                        <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,2)">
                                                            <i class="fa fa-expand"></i>
                                                        </a>
                                                    </div>
                                                </div>

                                                <div class="item-slick3" data-thumb="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,3)">
                                                    <div class="wrap-pic-w pos-relative">
                                                        <img src="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,3)" alt="IMG-PRODUCT">

                                                        <a class="flex-c-m size-108 how-pos1 bor0 fs-16 cl10 bg0 hov-btn3 trans-04" href="@HtmlHelperExtensions.ImageBookOrDefault(item.SachID,3)">
                                                            <i class="fa fa-expand"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6 col-lg-5 p-b-30">
                                    <div class="p-r-50 p-t-5 p-lr-0-lg">
                                        <h4 class="mtext-105 cl2 js-name-detail p-b-14">
                                            @item.TenSach
                                        </h4>


                                        @if (item.GiaKhuyenMai == null)
                                        {
                                            <span class="mtext-106 cl2">
                                                @item.GiaTien.ToString("N0")
                                            </span>

                                        }
                                        else
                                        {
                                            <span class="mtext-106 cl2">
                                                @item.GiaKhuyenMai.GetValueOrDefault(0).ToString("N0")
                                            </span>
                                            <p>
                                                Tiết kiệm: <a style="color: red">@HtmlHelperExtensions.ConvertDecimaltoIntegerSalePrice(item.GiaKhuyenMai.GetValueOrDefault(0), item.GiaTien) %<a> @(item.GiaTien - item.GiaKhuyenMai) ₫
                                            </p>
                                            <p>
                                                Giá thị trường: @item.GiaTien.ToString("N0") ₫
                                            </p>
                                        }


                                        <p class="stext-102 cl3 p-t-23">
                                            @item.MoTa
                                        </p>

                                        <!--  -->
                                        <div class="p-t-33">

                                            <div class="flex-w flex-r-m p-b-10">

                                                <div class="size-204 flex-w flex-m respon6-next">
                                                    @using (Html.BeginForm("AddItem", "Cart", new { id = item.SachID }, FormMethod.Post))
                                                    {
                                                        <div class="wrap-num-product flex-w m-r-20 m-tb-10">
                                                            <div class="btn-num-product-down cl8 hov-btn3 trans-04 flex-c-m">
                                                                <i class="fs-16 zmdi zmdi-minus"></i>
                                                            </div>

                                                            <input class="mtext-104 cl3 txt-center num-product" type="number" name="quantity" id="quantity" value="1" max="@item.SoLuong">

                                                            <div class="btn-num-product-up cl8 hov-btn3 trans-04 flex-c-m">
                                                                <i class="fs-16 zmdi zmdi-plus"></i>
                                                            </div>
                                                        </div>

                                                        if (item.SoLuong > 0)
                                                        {
                                                            <button class="flex-c-m stext-101 cl0 size-101 bg1 bor1 hov-btn1 p-lr-15 trans-04 js-addcart-detail" type="submit">
                                                                Chọn mua
                                                            </button>
                                                        }
                                                        else
                                                        {
                                                            <span class="btn-default">
                                                                Hết hàng
                                                            </span>
                                                        }
                                                    }
                                                </div>

                                            </div>
                                        </div>

                                        <!--  -->
                                        <div class="flex-w flex-m p-l-100 p-t-40 respon7">
                                            <div class="flex-m bor9 p-r-10 m-r-11">
                                                <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 js-addwish-detail tooltip100" data-tooltip="Add to Wishlist">
                                                    <i class="zmdi zmdi-favorite"></i>
                                                </a>
                                            </div>

                                            <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 m-r-8 tooltip100" data-tooltip="Facebook">
                                                <i class="fa fa-facebook"></i>
                                            </a>

                                            <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 m-r-8 tooltip100" data-tooltip="Twitter">
                                                <i class="fa fa-twitter"></i>
                                            </a>

                                            <a href="/Assets/Customer/cozastore/#" class="fs-14 cl3 hov-cl1 trans-04 lh-10 p-lr-5 p-tb-2 m-r-8 tooltip100" data-tooltip="Google Plus">
                                                <i class="fa fa-google-plus"></i>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            }
        }




        <div class="bor10 m-t-50 p-t-43 p-b-40">
            <!-- Tab01 -->
            <div class="tab01">
                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    <li class="nav-item p-b-10">
                        <a class="nav-link active" data-toggle="tab" href="#description" role="tab">GIỚI THIỆU SÁCH</a>
                    </li>

                    <li class="nav-item p-b-10">
                        <a class="nav-link" data-toggle="tab" href="#information" role="tab">THÔNG TIN CHI TIẾT</a>
                    </li>

                    <li class="nav-item p-b-10">
                        <a class="nav-link" data-toggle="tab" href="#reviews" role="tab">KHÁCH HÀNG NHẬN XÉT</a>
                    </li>
                </ul>

                <!-- Tab panes -->
                <div class="tab-content p-t-43">
                    <!-- - -->
                    <div class="tab-pane fade show active" id="description" role="tabpanel">
                        <div class="how-pos2 p-lr-15-md">
                            <p class="stext-102 cl6">
                                @Model.MoTa
                            </p>
                        </div>
                    </div>

                    <!-- - -->
                    <div class="tab-pane fade" id="information" role="tabpanel">
                        <div class="row">
                            <div class="col-sm-10 col-md-8 col-lg-6 m-lr-auto">
                                <ul class="p-lr-28 p-lr-15-sm">
                                   
                                    <li class="flex-w flex-t p-b-7">
                                        <span class="stext-102 cl3 size-205">
                                            Tác giả
                                        </span>

                                        <span class="stext-102 cl6 size-206">
                                            @Model.TacGia
                                        </span>
                                    </li>

                                    <li class="flex-w flex-t p-b-7">
                                        <span class="stext-102 cl3 size-205">
                                            Ngày xuất bản
                                        </span>

                                        <span class="stext-102 cl6 size-206">
                                            @Model.NgayXuatBan.ToString("MM-yyyy")
                                        </span>
                                    </li>
                                    
                                    <li class="flex-w flex-t p-b-7">
                                        <span class="stext-102 cl3 size-205">
                                            Nhà xuất bản
                                        </span>

                                        <span class="stext-102 cl6 size-206">
                                            @Model.NhaXuatBan.TenNXB
                                        </span>
                                    </li>
                                   
                                    <li class="flex-w flex-t p-b-7">
                                        <span class="stext-102 cl3 size-205">
                                            Loại bìa
                                        </span>

                                        <span class="stext-102 cl6 size-206">
                                            @Model.LoaiBia.LoaiBia1
                                        </span>
                                    </li>
                                    <li class="flex-w flex-t p-b-7">
                                        <span class="stext-102 cl3 size-205">
                                            Số trang
                                        </span>

                                        <span class="stext-102 cl6 size-206">
                                            @Model.SoTrang
                                        </span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <!-- - -->
                    <div class="tab-pane fade" id="reviews" role="tabpanel">
                        <div class="row">
                            <div class="col-sm-10 col-md-8 col-lg-6 m-lr-auto">
                                <div class="p-b-30 m-lr-15-sm">

                                    <!-- Add review -->

                                    @using (Html.BeginForm("Rating", "ListBooks", new { id = Model.SachID }, FormMethod.Post))
                                    {

                                        <form class="w-full">
                                            @if (muahang == 1)
                                            {
                                                <h5 class="mtext-108 cl2 p-b-7">
                                                    Viết nhận xét của bạn
                                                </h5>

                                                <div class="flex-w flex-m p-t-50 p-b-23">
                                                    <span class="stext-102 cl3 m-r-16">
                                                        Đánh giá của bạn về sản phẩm này
                                                    </span>

                                                    <span class="wrap-rating fs-18 cl11 pointer">
                                                        <i class="item-rating pointer zmdi zmdi-star-outline"></i>
                                                        <i class="item-rating pointer zmdi zmdi-star-outline"></i>
                                                        <i class="item-rating pointer zmdi zmdi-star-outline"></i>
                                                        <i class="item-rating pointer zmdi zmdi-star-outline"></i>
                                                        <i class="item-rating pointer zmdi zmdi-star-outline"></i>
                                                        <input class="dis-none" type="number" name="rating">

                                                    </span>
                                                </div>

                                                <div class="row p-b-25">

                                                    <div class="col-sm-6 p-b-5">
                                                        <label class="stext-102 cl3" for="email">Tiêu đề</label>
                                                        <input class="size-111 bor8 stext-102 cl2 p-lr-20" id="title" type="text" name="title">
                                                    </div>
                                                    <div class="col-12 p-b-5">
                                                        <label class="stext-102 cl3" for="review">Nội dung nhận xét</label>
                                                        <textarea class="size-110 bor8 stext-102 cl2 p-lr-20 p-tb-10" id="review" name="review"></textarea>
                                                    </div>
                                                </div>


                                                if (ktlogin == 0)
                                                {
                                                    <h5 class="mtext-108 cl2 p-b-7">
                                                        Bạn chưa đăng nhập, Phải đăng nhập trước khi nhận xét
                                                    </h5>
                                                    <button class="flex-c-m stext-101 cl0 size-112 bg7 bor11 p-lr-15 trans-04 m-b-10" disabled>
                                                        Gửi nhận xét
                                                    </button>
                                                }
                                                else if (danhanxet == 1)
                                                {
                                                    <h5 class="mtext-108 cl2 p-b-7">
                                                        Bạn chỉ được đánh giá 1 lần!
                                                    </h5>
                                                    <button class="flex-c-m stext-101 cl0 size-112 bg7 bor11 p-lr-15 trans-04 m-b-10" disabled>
                                                        Gửi nhận xét
                                                    </button>
                                                }
                                                else
                                                {
                                                    <button class="flex-c-m stext-101 cl0 size-112 bg7 bor11 hov-btn3 p-lr-15 trans-04 m-b-10" type="submit">
                                                        Gửi nhận xét
                                                    </button>
                                                }
                                            }
                                           
                                            else
                                            {
                                                <h5 class="mtext-108 cl2 p-b-7">
                                                    Bạn phải mua sản phẩm này mới được nhận xét
                                                    
                                                </h5>
                                            }


                                        </form>
                                    }

                                    <br>

                                    <!-- Review -->
                                    @if (Comment.Count == 0)
                                    {
                                        <p>Chưa có nhận xét</p>
                                    }
                                    else
                                    {
                                        <h5 class="mtext-108 cl2 p-b-7">
                                            Các đánh giá về sản phẩm:
                                        </h5>
                                        <br />
                                        <div class="flex-w flex-t p-b-50">
                                            @foreach (var item in Comment)
                                            {
                                                var sosao = item.SoSao;
                                                <div class="size-207">
                                                    <div class="flex-w flex-sb-m p-b-0">
                                                        <span class="mtext-107 cl2 p-r-20">
                                                            @item.Customer.HoTen
                                                        </span>
                                                        <span class="fs-18 cl11">
                                                            @for (int i = 0; i < 5; i++)
                                                            {
                                                                if (sosao > 0)
                                                                {
                                                                    <i class="zmdi zmdi-star"></i>
                                                                    sosao--;
                                                                }
                                                                else
                                                                {
                                                                    <i class="zmdi zmdi-star-outline"></i>
                                                                }
                                                            }
                                                        </span>
                                                    </div>
                                                    <p>
                                                        @item.NoiDung
                                                    </p>
                                                    <br />
                                                </div>

                                            }
                                        </div>

                                    }


                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    
</section>

